{
    "version": "1.0",
    "defaultProvider": "cdnjs",
    "libraries": [
        {
            "library": "jquery@3.3.1",
            "destination": "wwwroot/lib/jquery/"
        },
        {
            "library": "twitter-bootstrap@4.3.1",
            "destination": "wwwroot/lib/bootstrap/"
        },
        {
            "provider": "unpkg",
            "library": "@fullcalendar/core@4.3.1",
            "destination": "wwwroot/lib/fullcalendar/core/",
            "files": [
                "main.css",
            ]
        },
        {
            "provider": "unpkg",
            "library": "@fullcalendar/daygrid@4.3.0",
            "destination": "wwwroot/lib/fullcalendar/daygrid/",
            "files": [
                "main.css"
            ]
        },
        {
            "provider": "unpkg",
            "library": "@fullcalendar/timegrid@4.3.0",
            "destination": "wwwroot/lib/fullcalendar/timegrid/",
            "files": [
                "main.css"
            ]
        }
    ]
}
