{
    "version": "1.0",
    "defaultProvider": "cdnjs",
    "libraries": [
        {
            "library": "jquery@3.3.1",
            "destination": "wwwroot/lib/jquery/"
        },
        {
            "library": "twitter-bootstrap@4.3.1",
            "destination": "wwwroot/lib/bootstrap/"
        },
        {
            "library": "underscore.js@1.9.1",
            "destination": "wwwroot/lib/underscore/"
        },
        {
            "library": "js-joda@1.11.0",
            "destination": "wwwroot/lib/js-joda/"
        },
        {
            "library": "tinycolor@1.4.1",
            "destination": "wwwroot/lib/tinycolor/"
        },
        {
            "provider": "unpkg",
            "library": "@fullcalendar/core@4.3.1",
            "destination": "wwwroot/lib/fullcalendar/core/"
        },
        {
            "provider": "unpkg",
            "library": "@fullcalendar/resource-timegrid@4.3.0",
            "destination": "wwwroot/lib/fullcalendar/resource-timegrid/"
        },
        {
            "provider": "unpkg",
            "library": "@fullcalendar/resource-common@4.3.1",
            "destination": "wwwroot/lib/fullcalendar/resource-common/"
        },
        {
            "provider": "unpkg",
            "library": "@fullcalendar/resource-daygrid@4.3.0",
            "destination": "wwwroot/lib/fullcalendar/resource-daygrid/"
        },
        {
            "provider": "unpkg",
            "library": "@fullcalendar/daygrid@4.3.0",
            "destination": "wwwroot/lib/fullcalendar/daygrid/"
        },
        {
            "provider": "unpkg",
            "library": "@fullcalendar/timegrid@4.3.0",
            "destination": "wwwroot/lib/fullcalendar/timegrid/"
        }
    ]
}
